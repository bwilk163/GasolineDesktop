<Window x:Class="GasolineDesktop.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GasolineDesktop"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="827.163">
    <Grid>
        <Grid Background="AliceBlue" HorizontalAlignment="Left" Height="419" VerticalAlignment="Top" Width="180">
            <Button Content="Dodaj stację" HorizontalAlignment="Left" VerticalAlignment="Top" Width="160" Margin="10,46,0,0" Click="AddStationButton_Click"/>
            <Button Content="Wyświetl stacje" HorizontalAlignment="Left" VerticalAlignment="Top" Width="160" Margin="10,71,0,0" Click="ViewStationsButton_Click"/>
            <Button Content="Wyświetl paliwa" HorizontalAlignment="Left" VerticalAlignment="Top" Width="160" Margin="10,96,0,0" Click="ViewFuelsTypesButton_Click"/>
        </Grid>

        <Frame x:Name="Main" Margin="180,0,0,0" Navigated="Main_Navigated"></Frame>

    </Grid>
</Window>
